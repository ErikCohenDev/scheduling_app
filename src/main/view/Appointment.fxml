<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500" prefWidth="1000" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.Appointment">
   <children>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <TableView fx:id="appointmentTable" layoutX="14.0" layoutY="50.0" prefHeight="401.0" prefWidth="975.0">
        <columns>
          <TableColumn fx:id="appointmentIdCol" prefWidth="37.0" text="ID" />
          <TableColumn fx:id="appointmentTitleCol" prefWidth="86.0" text="Title" />
            <TableColumn fx:id="appointmentDescriptionCol" prefWidth="123.0" text="Description" />
            <TableColumn fx:id="appointmentLocationCol" minWidth="0.0" prefWidth="108.0" text="Location" />
            <TableColumn fx:id="appointmentContactCol" prefWidth="126.0" text="Contact" />
            <TableColumn fx:id="appointmentTypeCol" minWidth="0.0" prefWidth="107.0" text="Type" />
            <TableColumn fx:id="appointmentURLCol" prefWidth="143.0" text="URL" />
            <TableColumn fx:id="appointmentStartCol" prefWidth="121.0" text="Start" />
            <TableColumn fx:id="appointmentEndCol" prefWidth="121.0" text="End" />
        </columns>
      </TableView>
       <Button layoutX="845.0" layoutY="459.0" mnemonicParsing="false" onAction="#add" prefHeight="27.0" prefWidth="144.0" text="Add" />
       <Button layoutX="687.0" layoutY="459.0" mnemonicParsing="false" onAction="#update" prefHeight="27.0" prefWidth="144.0" text="Modify" />
       <Button layoutX="526.0" layoutY="459.0" mnemonicParsing="false" onAction="#delete" prefHeight="27.0" prefWidth="144.0" text="Delete" />
      <RadioButton fx:id="byWeekRadio" layoutX="472.0" layoutY="19.0" mnemonicParsing="false" text="Week" />
      <RadioButton fx:id="byMonthRadio" layoutX="390.0" layoutY="19.0" mnemonicParsing="false" text="Month" />
   </children>
</AnchorPane>
